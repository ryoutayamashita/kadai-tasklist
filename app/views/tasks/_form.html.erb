<%= form_for(task) do |f| %>
<% if task.errors.any? %>

<div id="error_explanation">
  <ul>
    <% message.errors.full_messages.each do |message| %>
    <li>
      <%= task %>
    </li>
    <% end %>
  </ul>
</div>
<% end %>

<%= f.label :title,"タイトル" %>
<%= f.text_field :title %>
<%= f.label :content,"タスク" %>
<%= f.text_field :content %>
<%= f.submit "追加" %>
<% end %>
